#: tracdragdrop/htdocs/tracdragdrop.js:122
msgid "Paste an image to attach"
msgstr "添付する画像をペースト"

#: tracdragdrop/htdocs/tracdragdrop.js:170
#: tracdragdrop/htdocs/tracdragdrop.js:192
msgid "No available image on your clipboard"
msgstr "クリップボードに使用できる画像がありません"

#: tracdragdrop/htdocs/tracdragdrop.js:213
#: tracdragdrop/htdocs/tracdragdrop.js:250
#, python-format
msgid "Cannot load an image from '%(src)s'."
msgstr "'%(src)s' から画像を読み込むことができません。"

#: tracdragdrop/htdocs/tracdragdrop.js:222
msgid ""
"You can attach an image from your clipboard directly with CTRL-V or "
"\"Paste\" on the context menu."
msgstr "CTRL-V や右クリックメニューの貼り付けでクリップボードの画像を直接添付できます。"

#: tracdragdrop/htdocs/tracdragdrop.js:287
#, python-format
msgid "Exceeded maximum allowed file size (%(size)s bytes)"
msgstr "最大ファイルサイズを超えています (%(size)sバイト)"

#: tracdragdrop/htdocs/tracdragdrop.js:291
msgid "Can't upload empty file"
msgstr "空のファイルはアップロードできません"

#: tracdragdrop/htdocs/tracdragdrop.js:317
msgid "Canceled"
msgstr "キャンセルしました"

#: tracdragdrop/htdocs/tracdragdrop.js:340
#, python-format
msgid "Uploaded %(percent)s%%"
msgstr "%(percent)s%%アップロード"

#: tracdragdrop/htdocs/tracdragdrop.js:367
#: tracdragdrop/htdocs/tracdragdrop.js:570
msgid "Uploading..."
msgstr "アップロード中…"

#: tracdragdrop/htdocs/tracdragdrop.js:379
msgid "Timed out"
msgstr "タイムアウトしました"

#: tracdragdrop/htdocs/tracdragdrop.js:382
msgid "Aborted"
msgstr "中断しました"

#: tracdragdrop/htdocs/tracdragdrop.js:470
msgid "Add Attachment"
msgstr "添付ファイルの追加"

#: tracdragdrop/htdocs/tracdragdrop.js:502
msgid "You may use drag and drop here."
msgstr "ドラッグ・アンド・ドロップが使えます。"

#: tracdragdrop/htdocs/tracdragdrop.js:611
msgid "Delete attachment"
msgstr "添付ファイルの削除"

#: tracdragdrop/htdocs/tracdragdrop.js:617
#, python-format
msgid "Are you sure you want to delete '%(name)s'?"
msgstr "'%(name)s' を削除しますか?"

#: tracdragdrop/htdocs/tracdragdrop.js:651
msgid "TracLink"
msgstr "TracLink"

#: tracdragdrop/htdocs/tracdragdrop.js:651
msgid "Image macro"
msgstr "Image マクロ"

#: tracdragdrop/htdocs/tracdragdrop.js:712
msgid "Drop files to attach"
msgstr "添付するファイルをドロップ"

