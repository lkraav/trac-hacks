#, python-format
msgid ""
"Line %(num)d: Action names can contain only alphabetic and numeric "
"characters."
msgstr "%(num)d行目: アクション名には英数字のみが使えます。"

#: tracworkflowadmin/web_ui.py:328
#, python-format
msgid "Line %(num)d: Action name is duplicated. The name must be unique."
msgstr "%(num)d行目: アクション名が重複しています。一意でなければなりません。"

